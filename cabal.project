import: https://www.stackage.org/nightly-2025-02-25/cabal.config

packages: 
  webdriver-precore/*.cabal
  webdriver-precore/test-server/*.cabal

with-compiler: ghc-9.10.1
-- with-compiler: ghc-9.10.1
-- with-compiler: ghc-9.8.4

-- always build tests without needing the --enable-tests flag
tests: True

-- allow cabal repl to load multiple packages at once
multi-repl: True